
# END Wordfence WAF
php_value max_execution_time = 180

# New landing

<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|webp|gif|js|swf|svg)$">
Header set Cache-Control "max-age=31536000, public"
</FilesMatch>

<FilesMatch "\.(css)$">
Header set Cache-Control "max-age=536000, public"
</FilesMatch>

#Alternate default index pagesffffff

DirectoryIndex index.html index.php

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{HTTP_HOST} ^book.earthen.io$ 
RewriteRule (.*)$ https://book.earthen.io/en/ [R=301,L]
</IfModule>



